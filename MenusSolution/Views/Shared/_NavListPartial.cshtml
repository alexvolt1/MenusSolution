@model System.Collections.Generic.IEnumerable<MenusSolution.Models.ViewModels.NavigationListViewModel>
@*@model System.Collections.Generic.IEnumerable<MenusSolution.Models.ViewModels.MenuViewModel>*@


@foreach (var menu in Model)
{
    if (menu.Children.Any())
    {
        <li class="treeview">
            <a href="#">
                <i class="@menu.Id"></i> <span>@menu.DefaultName</span>
                @*<i class="@menu.IconClass"></i> <span>@menu.Content</span>*@
                <span class="pull-right-container">
                    <i class="fa fa-angle-left pull-right"></i>
                </span>
            </a>
            <ul class="treeview-menu">
                @*@Html.Partial("_TestPartial", menu.Children)*@
                <partial name="_NavListPartial" model="menu.Children" />
            </ul>
        </li>
    }
    else
    {
<li class="treeview">
    <a href="@menu.ImageUrl">
    @*<a href="@menu.Href">*@
        <i class="@menu.Id"></i> <span>@menu.DefaultName</span>
        @*<i class="@menu.IconClass"></i> <span>@menu.Content</span>*@
    </a>
</li>
    }
}

